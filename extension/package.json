{
  "name": "class-browser",
  "displayName": "Class Browser",
  "description": "Class Browser",
  "publisher": "jarusll",
  "version": "1.0.1",
  "icon": "media/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/jarusll/vscode-class-browser"
  },
  "license" : "MIT",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:vs-class-browser"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vs-class-browser-view",
          "title": "Class Browser",
          "icon": "media/tree.svg"
        }
      ]
    },
    "views": {
      "vs-class-browser-view": [
        {
          "type": "webview",
          "id": "vs-class-browser",
          "name": "Browser",
          "icon": "media/tree.svg",
          "contextualTitle": ""
        }
      ]
    },
    "commands": [
      {
        "command": "search-class-browser",
        "category": "ClassBrowser",
        "title": "Search"
      },
      {
        "command": "open-class-browser",
        "category": "ClassBrowser",
        "title": "Open"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack --config ./build/node-extension.webpack.config.js",
    "watch": "concurrently \"rollup -c -w\" \"webpack --watch --config ./build/node-extension.webpack.config.js\"",
    "package": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-typescript": "^8.1.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/polka": "^0.5.1",
    "@types/vscode": "^1.52.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.15.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.31.0",
    "svelte-check": "^1.1.23",
    "svelte-preprocess": "^4.6.1",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "polka": "^0.5.2"
  }
}
